{
	"name": "sy/bootstrap",
	"description": "A library for starting to create a website",
	"type": "library",
	"require": {
		"php": ">=7",
		"sy/html": "dev-master",
		"sy/mysql": "^3.0",
		"sy/container": "^1",
		"sy/cache": "^1",
		"sy/mail": "^2",
		"sy/event": "^1",
		"hashids/hashids": "^4.1"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "Syone",
			"email": "syone7@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Sy\\Bootstrap\\": "src/"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^10"
	},
	"autoload-dev": {
		"psr-4": {
			"Sy\\Test\\": "tests/"
		}
	},
	"scripts": {
		"test": "phpunit --bootstrap tests/conf.php --coverage-text --coverage-filter src tests"
	}
}
